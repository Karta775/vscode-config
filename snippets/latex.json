{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Section": {
		"prefix": "sec",
		"body": [
			"\\section{$1}",
			"$0"
		],
		"description": "Create a level 1 section"
	},

	"Subsection": {
		"prefix": "ssec",
		"body": [
			"\\subsection{$1}",
			"$0"
		],
		"description": "Create a level 2 section"
	},

	"Subsubsection": {
		"prefix": "sssec",
		"body": [
			"\\subsubsection{$1}",
			"$0"
		],
		"description": "Create a level 3 section"
	},

	"Empty citation": {
		"prefix": "cc",
		"body": "\\cite{???} $0",
		"description": "Create an emtpy (unfilled) citation"
	},

	"Inline code": {
		"prefix": "co",
		"body": "\\code{$1} $0",
		"description": "Create an inline code snippet"
	}
}